<Window x:Class="Silver_HTPC.PrimeResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Silver_HTPC"
        mc:Ignorable="d"
        Title="PrimeResult" Height="1080" Width="1920" KeyDown="Window_KeyDown">
    <Window.Resources>
        <ScaleTransform x:Key="windowScaleTransform" ScaleX="2.4" ScaleY="2.4" />
    </Window.Resources>
    <Grid LayoutTransform="{StaticResource windowScaleTransform}">
        <Image HorizontalAlignment="Left" Height="437" VerticalAlignment="Top" Width="797" Source="Image/spiderverse_prime_video.jpg" Stretch="Fill"/>
    </Grid>
</Window>
